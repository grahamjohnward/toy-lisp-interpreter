{
    "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
    "editor.formatOnSave": true,
    "C_Cpp.clang_format_style": "{ BasedOnStyle: WebKit, PointerAlignment: Right, AllowShortBlocksOnASingleLine: false, AllowShortFunctionsOnASingleLine: false }",
    "cmake.generator": "Ninja",
    "files.associations": {
        "*.tcc": "c",
        "functional": "c",
        "fstream": "c",
        "istream": "c",
        "streambuf": "c",
        "ostream": "c",
        "array": "c",
        "string_view": "c",
        "initializer_list": "c",
        "utility": "c",
        "stddef.h": "c",
        "lisp.h": "c",
        "tuple": "c",
        "random": "c",
        "sstream": "c",
        "text_stream.h": "c",
        "string": "c"
    }
}